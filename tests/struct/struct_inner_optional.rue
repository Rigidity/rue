struct Value {
    inner?: Value,
}

fun main() -> Nil {
    let value = Value {
        inner: Value {
            inner: Value {
                inner: Value {
                    inner: Value {}
                }
            }
        }
    };

    assert value.inner?;
    assert value.inner.inner?;
    assert value.inner.inner.inner?;
    assert value.inner.inner.inner.inner?;
    assert !value.inner.inner.inner.inner.inner?;

    nil
}
