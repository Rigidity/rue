struct Point {
    x: Int,
    y: Int,
    z?: Int,
}

fun main() -> Int {
    let point_xy = Point { x: 42, y: 34 };
    let point_xyz = Point { x: 42, y: 34, z: 69 };
    sum(point_xy) + sum(point_xyz)
}

fun sum(point: Point) -> Int {
    if point.z? {
        point.x + point.y + point.z
    } else {
        point.x + point.y
    }
}
